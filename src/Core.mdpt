            e1 ::= True
        else if not args.\1 .? i then
            False
        else
            e2 ::= (args.\0.i in_order args.\1.i);

        e1 ::?= 'This is the case where LHS is a leading subsequence of or is equal to RHS.';
        e2 ::?= 'This will succeed iff in_order() is defined for the member type.';
