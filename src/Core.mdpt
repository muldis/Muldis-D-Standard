        ri ::= (args.\1.\0);
        repl_member ::= (args.\1.\1);
        fi ::= first_index src;
        li ::= last_index src;
        emp ::= empty src;
        <-- (if ri > fi then slice_range::(src, fi, --ri) else emp)
            insert repl_member
            catenate (if ri < li then slice_range::(src, ++ri, li) else emp);
