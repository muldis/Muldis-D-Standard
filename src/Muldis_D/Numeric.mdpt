MD.Numeric.Round_Meth.Half_Down ::= type of Singleton
    where (Capsule_type(.0) = $Half_Down);

MD.Numeric.Round_Meth.Half_Up ::= type of Singleton
    where (Capsule_type(.0) = $Half_Up);

MD.Numeric.Round_Meth.Half_To_Zero ::= type of Singleton
    where (Capsule_type(.0) = $Half_To_Zero);

MD.Numeric.Round_Meth.Half_To_Inf ::= type of Singleton
    where (Capsule_type(.0) = $Half_To_Inf);

MD.Numeric.Round_Meth.Half_Even ::= type of Singleton
    where (Capsule_type(.0) = $Half_Even);
